{
  "name": "@enhancer/auth-client",
  "version": "1.1.0",
  "description": "Node.js client library for Enhancer Auth Service",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./middleware/express": {
      "import": "./dist/middleware/express.mjs",
      "require": "./dist/middleware/express.js",
      "types": "./dist/middleware/express.d.ts"
    },
    "./middleware/fastify": {
      "import": "./dist/middleware/fastify.mjs",
      "require": "./dist/middleware/fastify.js",
      "types": "./dist/middleware/fastify.d.ts"
    },
    "./middleware/nextauth-provider": {
      "import": "./dist/middleware/nextauth-provider.mjs",
      "require": "./dist/middleware/nextauth-provider.js",
      "types": "./dist/middleware/nextauth-provider.d.ts"
    }
  },
  "files": ["dist", "README.md", "LICENSE"],
  "scripts": {
    "build": "bun run build.ts",
    "dev": "bun run build.ts --watch",
    "test": "bun test",
    "test:coverage": "bun test --coverage",
    "lint": "biome check ./src",
    "lint:fix": "biome check --write ./src",
    "format": "biome format --write ./src",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "bun run typecheck && bun run lint && bun run test && bun run build"
  },
  "keywords": ["auth", "oauth2", "jwt", "authentication", "enhancer", "typescript"],
  "author": "Enhancer",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "axios": "^1.7.0",
    "jsonwebtoken": "^9.0.2",
    "zod": "^3.23.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@types/bun": "latest",
    "@types/express": "^5.0.6",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^25.2.0",
    "fastify": "^4.26.0"
  },
  "peerDependencies": {
    "express": "^4.18.0",
    "fastify": "^4.26.0",
    "next-auth": "^5.0.0"
  },
  "peerDependenciesMeta": {
    "express": {
      "optional": true
    },
    "fastify": {
      "optional": true
    },
    "next-auth": {
      "optional": true
    }
  }
}
